{
    "name": "open-quiz-api",
    "version": "0.0.1",
    "description": "The open quiz api",
    "main": "index.js",
    "scripts": {
        "dev": "dotenv -e .env.development -- nodemon src/index.ts --devmode",
        "start": "node .",
        "prisma:push": "dotenv -e .env.development yarn prisma db push",
        "prisma:migrate": "dotenv -e .env.development yarn prisma migrate deploy",
        "postinstall": "npx prisma generate",
        "docker:up": "docker-compose up --detach",
        "docker:down": "docker-compose down",
        "pretest:ci": "yarn docker:up && yarn test:prisma:push",
        "test:ci": "yarn test:vitest",
        "test": "yarn test:ci || yarn docker:down",
        "posttest:ci": "yarn docker:down",
        "test:vitest": "dotenv -e .env.test -- vitest --threads false --run",
        "test:prisma:push": "dotenv -e .env.test yarn prisma db push",
        "test:coverage": "yarn test:ci --coverage"
    },
    "author": "Josh Jeffers",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^4.8.0",
        "express": "^4.18.2",
        "jose": "^4.11.2",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/node": "^18.11.18",
        "@types/supertest": "^2.0.12",
        "@vitest/coverage-c8": "^0.28.4",
        "dotenv": "^16.0.3",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "prisma": "^4.8.0",
        "rest-zod-prisma": "^1.0.5",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "vitest": "^0.28.4",
        "vitest-mock-extended": "^1.0.8"
    }
}
